    "pg": "^8.11.3",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^4.3.12",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "chai": "^5.1.0",
